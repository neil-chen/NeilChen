/* 
   TODO Clean up this page since some of the form layout changes during development
*/
#training-dialog {
  display: none;
}

.landing-page-steps p {
  font-size: 14px;
  color: black;
}

.next-page {
  margin-top: 15px;
}

.next-page .cancel {
  float: right;
  height: 29px;
  margin: 5px;
}
.next-page .next-btn {
  border: 1px solid rgb(72, 153, 194);
  float: right;
  margin: 5px;
  text-align: center;
  width: 80px;
}

.self_registration_form_wrapper, .reset-password-form-wrapper, .approving_form_wrapper, .landing-page-wrapper {
  margin: 0 auto;
  width: 950px;
}

.self_registration_header {
  border-bottom: 5px solid;
  margin-bottom: 20px;
  padding-bottom: 15px;
  width: 93%;
}
.reset-password-header, .landing-page-header {
  border-bottom: 5px solid;
  margin-bottom: 20px;
  padding-bottom: 15px;
  width: 100%;
}

.left_page, .right_page {
  width: 50%;
  float: left;
  position: relative;
}

.bottom_page {
  border-top: 1px dashed;
  clear: both;
  float: left;
  margin-top: 20px;
  width: 100%;
}

.bottom_page #edit-trainning-date-wrapper {
  width: 100%;
}

.bottom_page .form-item label {
  clear: left;
}

#edit-covidien-user-wrapper label {
  float: left;
  width: 160px;
}

#edit-covidien-user-wrapper input[type="checkbox"] {
  float: right;
}

.left_page .form-item, .right_page .form-item {
  padding: 15px 5px 5px;
}
.self_registration_form_wrapper .form-item label {
  color: #333333;
  float: left;
  font-family: "Arial Bold","Arial";
  font-size: 13px;
  font-style: normal;
  font-weight: 700;
  width: 30%;
}

.self_registration_form_wrapper .form-item label.error { 
  width: 41%;
  margin-left: 145px !important;
}

.self_registration_form_wrapper #edit-cov-user-wrapper label {
  width: 100%;
  margin-bottom: 15px;
}
  

.self_registration_form_wrapper input[type="text"],
.self_registration_form_wrapper #edit-last-name-wrapper input[type="text"] {
  width: 210px;
}

.self_registration_form_wrapper select {
  width: 218px;
}

#cot-info-wrapper fieldset {
  padding: 0px;
}


/*Bottom page with COT table*/
.columns-wrapper {
  width: 21%;
  float: left;
}

.columns-wrapper .form-item label {
  width: 100%;
}
 

#training-date-wrapper .form-item {
  width: 100%;
}

#devices-wrapper .form-item label {
  margin: 0 0 12px;
  text-align: left;
}
/*
.bottom_page .form-item {
    padding: 0 5px 10px;
}*/

#trainning-certificate-wrapper .form-item  {
  padding: 0 5px 8px;
}

#trainning-certificate-wrapper .form-item input {
  font-size: 10px;
    height: 15px;
    margin: 0;
}

#cot-info-wrapper .columns-wrapper {
  text-align: center;
}

#edit-cot-files-upload {
   float: right;
    margin-bottom: 30px;
    margin-right: 154px;
    margin-top: 10px;
}

/*device rows*/
.devices-row {
  border-bottom: 1px solid;
  border-left: 1px solid;
  border-right: 1px solid;
  float: left;
  width: 100%;
}

.devices-row.first {
  border-top: 1px solid;
  margin-top: 15px;
}

.devices-row.first .form-item {
  border-left: 1px solid;
    float: left;
    font-weight: bold;
    text-align: center;
    width: 20%;
}

.devices-row .form-item .description {
  display: none;
}

.devices-row fieldset .form-item {
 clear: none;
    float: left !important;
    padding-bottom: 5px;
    padding-left: 5px; 
    padding-top: 5px;
    width: 20%;
}

.devices-row .form-item label {
  width: 100%;
}

.devices-row .container-inline-date .form-item, .container-inline-date .form-item input[type="text"] {
  float: left;
  height: 23px;
  padding: 0 0 0 15px !important;
  width: 100%;
}

.devices-row select {
    width: 175px;
}

.form-reset {
  border-radius: 5px;
  background: url("../images/blue_up.png") repeat scroll 50% 9% rgba(0, 0, 0, 0);
}

.form-reset:hover {
  border: 1px solid;
}

.files-wrapper-now {
  margin-top: 15px;
}

#cot-info-wrapper {
  margin-top: 15px
}

/*Complete Page*/
.complete {
  color: #333333;
  font-family: "Arial Regular","Arial";
  font-size: 13px;
  font-style: normal;
  font-weight: 400;
  line-height: normal;
  text-align: left;
}

.submit_wrapper {
  clear: both; 
  padding-top: 15px;
