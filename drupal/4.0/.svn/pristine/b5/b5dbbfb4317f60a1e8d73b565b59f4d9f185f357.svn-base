  width: 50%;
}
 
#edit-class-of-trade-wrapper, #proxy-wrapper {
  width: 50%;
  float: left;
}

#edit-class-of-trade-wrapper label {
  width: 30%;
  float: left;
}

#proxy-wrapper label { 
  float: left;
}

#proxy-wrapper label.error {
  margin-left: 45px;
}

#proxy-config-form input[type="submit"] {
